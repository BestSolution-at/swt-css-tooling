	 * Color definition 
	 */
	 color = <named-color> | <hex-color> | <rgb-color> | inherit;
	
	/**
	 * No whitespace is allowed between the number and the percent symbol.
	 * These are a percentage of some length, typically to the width or height of a node.
	 */
	percentage = @NUM ~ %;
	
	/**
	 * &lt;address&gt; can be an absolute URI, for example:
	 * <ul>
	 * <li>url(http://example.com)</li>
	 * <li>url('http://example.com')</li>
	 * <li>url("http://example.com")</li>
	 * </ul>
	 * or it can be relative to the location of the CSS file.
	 */
	uri = @URL;
	
	string = @STRING;
	
	bool = true | false;
	
	/**
	 * Linear or radial gradient for example:
	 * <table>
	 * <tr><td><span style="display:block;width:16px;height:16px;border:2px solid black;background: linear-gradient(to right, white, black);"></span></td><td>gradient #FFF #000 100%</td></tr>
	 * <tr><td><span style="display:block;width:16px;height:16px;border:2px solid black;background: linear-gradient(to bottom, white, black);"></span></td><td>gradient #FFF #000 100% false</td></tr>
	 * <tr><td><span style="display:block;width:16px;height:16px;border:2px solid black;background: linear-gradient(to right, red, green 50%, blue 100% );"></span></td><td>gradient red green blue 50% 100%</td></tr>
	 * </table>  
	 */
	gradient = [[linear|radial] gradient]? <color> <color>+ <percentage>+ <bool>?;
	
	/**
	 * A solid color or a gradient 
	 */
	color-fill = <color> | <gradient>;
	/**
