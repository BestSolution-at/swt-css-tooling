		 */
		swt-simple <bool>;
		/**
		 * Make the maximize icon visible
		 */
		swt-maximize-visible <bool>;
		/**
		 * Make the minimize icon visible
		 */
		swt-minimize-visible <bool>;
		/**
		 * Shown the mru
		 */
		swt-mru-visible <bool>;
		/**
		 * Set the maximize state
		 */
		swt-maximized <bool>;
		/**
		 * Set the minimize state
		 */
		swt-minimized <bool>;
		/**
		 * The height of the tabs
		 */
		swt-tab-height @NUM;
		/**
		 * Only one tab is shown at a time
		 */
		swt-single <bool>;
		/**
		 * <code>true</code> makes the close icon visible even on unselected tabs
		 */
		swt-unselected-close-visible <bool>;
		/**
		 * <code>true</code> makes the close icon visible even on unselected tabs
		 */
		swt-unselected-image-visible <bool>;
		/**
		 * Color used by the selected tab
		 */
		swt-selected-tabs-background <color-fill>;
		/**
		 * Enable showing of a drop shadow (depends on the renderer used)
		 */
		swt-shadow-visible <bool>;
		swt-selected-tab-fill <color-fill>;
		swt-tab-outline <color>;
		/**
		 * Color used for the shadow
		 */
		swt-shadow-color <color>;
		/**
		 * The padding to apply to the content
		 * <ul>
		 * <li>1 value: top, right, bottom, left get the same value</li>
		 * <li>4 values: 1st value is used for top, 2nd value is used for right, 3rd value is used for bottom, 4th value is used for left</li>
		 * </ul>
		 */
		padding @NUM | <padding-top> <padding-right> <padding-bottom> <padding-left>;
		/**
		 * the value used for the left padding 
		 */
		padding-left @NUM;
		/**
		 * the value used for the right padding 
		 */
		padding-right @NUM;
		/**
		 * the value used for the top padding 
		 */
		padding-top @NUM;
		/**
		 * the value used for the bottom padding 
		 */
		padding-bottom @NUM;
