	/**
	 * Base class for all windowed user interface classes 
	 */
	Control extends Widget {
		/**
		 * Background fill
		 */
		background <color-fill>;
		/**
		 * Background fill
		 */
		background-color <color-fill>;
		/**
		 * Background image
		 */
		background-image <uri>;
		/**
		 * Cursor shown when mouse is over the control e.g. <code>cursor: crosshair</code>
		 */
		cursor [ default | auto | crosshair | pointer | move | e-resize | ne-resize | nw-resize | n-resize | 
				 se-resize | sw-resize | s-resize | w-resize | text | wait | help ];
		/**
		 * Hide or show the control
		 */
		visibility [ hidden | visible ];
		/**
		 * The foreground color of the control used when displaying text e.g. <code>color: #FF0000</code>
		 */
		color <color>;
		/**
		 * The font used by the control when displaying text e.g. <code>font-family: "Terminal"</code>
		 */
		font-family <string>;
		/**
		 * The font size used by the control when displaying text e.g. <code>font-size: 12;</code>
		 */
		font-size @NUM;
		/**
		 * The font style used by the control when displaying text e.g. <code>font-style: italic;</code>
		 */
		font-style italic;
		/**
		 * The font weight used by the control when displaying text e.g. <code>font-weight: bold;</code>
		 */
		font-weight bold;
		/**
		 * The font used when displaying text e.g. <code>font: italic 12 bold "Terminal"</code>
		 */
		font <font-style> <font-size> <font-weight> <font-family>;
		/**
		 * Top margin used when the control is embedded in a GridLayout
		 */
		margin-top @NUM;
		/**
		 * Bottom margin used when the control is embedded in a GridLayout
		 */
		margin-bottom @NUM;
		/**
		 * Left margin used when the control is embedded in a GridLayout
		 */
		margin-left @NUM;
		/**
		 * Right margin used when the control is embedded in a GridLayout
		 */
		margin-right @NUM;
		/**
		 * Margin used when the control is embedded in a GridLayout
		 * <ul>
		 * <li>1 value: top, right, bottom, left get the same value</li>
		 * <li>2 values: 1st value is used for top &amp; bottom, 2nd value is used for left &amp; right</li>
		 * <li>3 values: 1st value is used for top, 2nd value is used for right &amp; left, 3rd value is used for bottom</li>
		 * <li>4 values: 1st value is used for top, 2nd value is used for right, 3rd value is used for bottom, 4th value is used for left</li>
		 * </ul>
		 */
		margin [@NUM] | [@NUM @NUM] | [@NUM @NUM @NUM] | [<margin-top> <margin-right> <margin-bottom> <margin-left>];
	}
	/**
	 * Represents a selectable user interface object
	 * <ul>
	 * <li>Push button</li>
	 * <li>Checkbox</li>
	 * <li>Radiobox</li>
	 * <li>Arrow button</li>
	 * </ul>
	 */
	Button extends Control {
		/**
		 * Transforms the text set on the control
		 */
		text-transform [capitalize | uppercase | lowercase | inherit ];
		/**
		 * Alignment of the text
		 */
		text-align [ left | center | right ];
		/**
		 * Alignment of the text, up &amp; down only apply when the button is created with SWT.ARROW
		 */
		swt-alignment [lead | trail | up | down ];
	}
	/**
	 * Represents an non-selectable user interface object
	 */
